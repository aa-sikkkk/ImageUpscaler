{
    "input_directory": "imageUpscaler/input_folder/",
    "output_directory": "imageUpscaler/output/",
    "upscale_factor": 1.0,
    "contrast_factor": 1.0,
    "color_factor": 1.0,
    "watermark_text": "",
    "watermark_position": "bottom_right",
    "format_conversion": "JPEG",
